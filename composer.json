{
    "name": "ankewk/easypay",
    "description": "a easy of chinese payment php sdk",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "ankewk",
            "email": "171640567@qq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "overtrue/wechat": "^4.0",
        "alipay/easysdk": "^2.2",
        "yansongda/pay": "~3.7.0"
    },

    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },

    "autoload": {
        "psr-4": {
            "Wxpay\\Payment\\": "src/Wxpay/Payment",
            "Alipay\\Payment\\": "src/Alipay/Payment",
            "Cmbpay\\Payment\\": "src/Cmbpay/Payment",
            "Douyinpay\\Payment\\": "src/Douyinpay/Payment",
            "Kuaishoupay\\Payment\\": "src/Kuaishoupay/Payment",
            "Jdpay\\Payment\\": "src/Jdpay/Payment",
            "Pinduoduopay\\Payment\\": "src/Pinduoduopay/Payment",
            "Meituanpay\\Payment\\": "src/Meituanpay/Payment",
            "Applepay\\Payment\\": "src/Applepay/Payment",
            "Huaweipay\\Payment\\": "src/Huaweipay/Payment"
        },
        "files": ["config/config_loader.php"]
    },
    "config": {
        "secure-http": false
    }
}
